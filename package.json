{
  "name": "eslint-config-suddi",
  "description": "Eslint configurations for Node.js, Angular.js and React.js development",
  "version": "4.0.16",
  "homepage": "https://www.npmjs.com/package/eslint-config-suddi",
  "author": {
    "name": "Sudharshan Ravindran",
    "email": "mail@suddi.io",
    "url": "https://suddi.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suddi/eslint-config-suddi"
  },
  "bugs": {
    "url": "https://github.com/suddi/eslint-config-suddi/issues"
  },
  "files": [
    "angular.js",
    "backend.js",
    "frontend.js",
    "node.js",
    "node-tab.js",
    "react.js"
  ],
  "main": "backend.js",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin-react",
    "eslint-plugin-angular",
    "react",
    "angular",
    "javascript",
    "rules"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 2.0.0"
  },
  "scripts": {
    "check-coverage": "istanbul check-coverage",
    "coverage": "istanbul cover _mocha -- --opts test/options/default.opts",
    "doctest": "mocha --opts test/options/doctest.opts",
    "lint": "eslint .",
    "prepush": "npm test",
    "security-check": "nsp check --output summary",
    "start": "ntl",
    "test": "npm run lint && npm run security-check && npm run coverage && npm run check-coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^4.0.0",
    "eslint-plugin-angular": "^3.0.0",
    "eslint-plugin-react": "^7.0.0",
    "husky": "^0.14.0",
    "istanbul": "^0.4.0",
    "jsdoctest": "^1.0.0",
    "mocha": "^5.1.0",
    "nsp": "^2.0.0",
    "ntl": "^1.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-angular": "^3.0.0",
    "eslint-plugin-react": "^7.0.0"
  },
  "peerDependencies": {
    "eslint": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "./frontend.js"
  }
}
